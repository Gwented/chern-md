bind-> main.chrn

var->
    name: str(!isEmpty, #rm_if 0..=5)
    age: u8 (5..=5)
    cat: %Cat

nest->
    .Cat:= {
        Dog dog = new Dog(); ∀ i ∈ ℕ
        color: %Color
    }

default_override->
    with JAVA:= {
        .name = i32,
        .age = i16
    }

complex_rules->
    .Color:= {Red, Green, Blue, Purple, Yellow}
